<?xml version='1.0' encoding='utf-8'?>
<widget id="com.areafor.intermezzo" version="1.7.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Intermezzo</name>
    <description>Intermezzo  Coros a la carta</description>
    <author email="info@miguelmanchego.com" href="miguelmanchego.com">
        Areafor
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <access origin="http://miguelmanchego.com/*" subdomains="true" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="false" />
    <preference name="target-device" value="universal" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="HideKeyboardFormAccessoryBar" value="true" />
    <preference name="SuppressesIncrementalRendering" value="true" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-maxSdkVersion" value="28" />
    <preference name="android-installLocation" value="auto" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="ErrorUrl" value="" />
    <preference name="BackgroundColor" value="0x000000" />
    <preference name="KeepRunning" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="LoadUrlTimeoutValue" value="20000" />
    <preference name="disable-cursor" value="true" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" height="36" src="res/icon/android/icon-36-ldpi.png" width="36" />
        <icon density="mdpi" height="48" src="res/icon/android/icon-48-mdpi.png" width="48" />
        <icon density="hdpi" height="72" src="res/icon/android/icon-72-hdpi.png" width="72" />
        <icon density="xhdpi" height="96" src="res/icon/android/icon-96-xhdpi.png" width="96" />
        <icon density="xxhdpi" height="144" src="res/icon/android/icon-144-xxhdpi.png" width="144" />
        <icon density="xxxhdpi" height="192" src="res/icon/android/icon-192-xxxhdpi.png" width="192" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <resource-file src="GoogleService-Info.plist" />
        <icon height="57" src="res/icon/ios/icon-57.png" width="57" />
        <icon height="114" src="res/icon/ios/icon-57-2x.png" width="114" />
        <icon height="72" src="res/icon/ios/icon-72.png" width="72" />
        <icon height="144" src="res/icon/ios/icon-72-2x.png" width="144" />
        <icon height="120" src="res/icon/ios/icon-120.png" width="120" />
        <icon height="76" src="res/icon/ios/icon-76.png" width="76" />
        <icon height="152" src="res/icon/ios/icon-152.png" width="152" />
        <icon height="40" src="res/icon/ios/icon-40.png" width="40" />
        <icon height="1024" src="res/icon/ios/icon-1024.png" width="1024" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.2" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="phonegap-plugin-push" spec="^2.1.3">
        <variable name="FCM_VERSION" value="11.6.2" />
    </plugin>
    <splash height="480" platform="ios" src="res/screen/ios/screen-iphone-portrait.png" width="320" />
    <splash height="960" platform="ios" src="res/screen/ios/screen-iphone-portrait-2x.png" width="640" />
    <splash height="1136" platform="ios" src="res/screen/ios/screen-iphone-portrait-568h-2x.png" width="640" />
    <splash density="mdpi" platform="android" src="res/screen/android/screen-mdpi-portrait.png" />
    <splash density="hdpi" platform="android" src="res/screen/android/screen-hdpi-portrait.png" />
    <splash density="xhdpi" platform="android" src="res/screen/android/screen-xhdpi-portrait.png" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <engine name="ios" spec="^4.5.5" />
    <engine name="android" spec="^7.0.0" />
</widget>
